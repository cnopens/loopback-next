{
  "name": "@loopback/grpc",
  "version": "0.2.0",
  "description": "A gRPC extension for LoopBack Next",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc --copy-resources",
    "build:watch": "lb-tsc --copy-resources --watch",
    "clean": "lb-clean loopback-grpc*.tgz dist package api-docs *.tsbuildinfo",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/grpc"
  },
  "author": "IBM Corp.",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "compilers",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.4.1",
    "@loopback/core": "^2.9.3",
    "@loopback/repository": "^2.11.0",
    "@loopback/rest": "^6.0.0",
    "debug": "^4.1.1",
    "glob": "^7.1.6",
    "grpc": "^1.24.3",
    "grpc_tools_node_protoc_ts": "^4.1.3",
    "protobufjs": "^6.10.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.0",
    "@loopback/eslint-config": "^9.0.0",
    "@loopback/testlab": "^3.2.2",
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.2"
  }
}
